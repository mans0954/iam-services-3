<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <mapping>
        <class-a>org.openiam.am.srvc.domain.AuthProviderEntity</class-a>
        <class-b>org.openiam.am.srvc.dto.AuthProvider</class-b>
        <field-exclude>
            <a>providerAttributeSet</a>
            <b>providerAttributeSet</b>
        </field-exclude>
        <field-exclude>
            <a>resource</a>
            <b>resource</b>
        </field-exclude>
    </mapping>

    <mapping>
        <class-a>org.openiam.am.srvc.domain.AuthResourceAttributeEntity</class-a>
        <class-b>org.openiam.am.srvc.dto.AttributeMap</class-b>
        <field>
            <a>attributeMapId</a>
            <b>attributeMapId</b>
        </field>
        <field>
            <a>resourceId</a>
            <b>resourceId</b>
        </field>
        <field>
            <a>amAttributeName</a>
            <b>accessManagerAttributeName</b>
        </field>
        <field>
            <a>amPolicyUrl</a>
            <b>policyUrl</b>
        </field>
        <field>
            <a>targetAttributeName</a>
            <b>targetAttributeName</b>
        </field>
        <field-exclude>
            <a>resource</a>
            <b>resource</b>
        </field-exclude>
    </mapping>
    <mapping>
        <class-a>org.openiam.idm.srvc.res.domain.ResourceEntity</class-a>
        <class-b>org.openiam.idm.srvc.res.dto.Resource</class-b>
        <field-exclude>
            <a>parentResources</a>
            <b>parentResources</b>
        </field-exclude>
        <field-exclude>
            <a>childResources</a>
            <b>childResources</b>
        </field-exclude>
        <field-exclude>
            <a>resourceRoles</a>
            <b>resourceRoles</b>
        </field-exclude>
        <field-exclude>
            <a>resourceProps</a>
            <b>resourceProps</b>
        </field-exclude>
        <field-exclude>
            <a>resourceGroups</a>
            <b>resourceGroups</b>
        </field-exclude>
        <field-exclude>
            <a>resourceUsers</a>
            <b>resourceUsers</b>
        </field-exclude>
    </mapping>

</mappings>