	<reporting>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<configuration>
					<doctitle>OpenIAM Identity Manager API v3</doctitle>
					<windowtitle>OpenIAM Java Documentation</windowtitle>
					<aggregate>true</aggregate>
					<stylesheet>maven</stylesheet>
				</configuration>
				<reportSets>
					<reportSet>
						<id>aggregate</id>
						<reports>
							<report>aggregate</report>
						</reports>
					</reportSet>
				</reportSets>
			</plugin>
		</plugins>
	</reporting>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.7.2</version>
				<configuration>
					<reportFormat>brief</reportFormat>
					<useFile>false</useFile>
					<includes>
						<include>**/*Test.java</include>
					</includes>
					<skipTests>${skipTests}</skipTests>
				</configuration>
			</plugin>
			<plugin>
				<groupId>pl.project13.maven</groupId>
				<artifactId>git-commit-id-plugin</artifactId>
				<version>${git.commit.id.plugin.version}</version>
				<executions>
					<execution>
						<phase>process-resources</phase>
						<goals>
							<goal>revision</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<dateFormat>${maven.build.timestamp.format}</dateFormat>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.3.1</version>
				<executions>
					<execution>
						<id>service</id>
						<phase>compile</phase>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<archive>
						<manifestEntries>
							<Organization>${pom.organization.name}</Organization>
							<Project>${pom.name}</Project>
							<Project-Version>${pom.version}.${git.commit.id.abbrev}</Project-Version>
							<Project-Git-Hash>${git.commit.id}</Project-Git-Hash>
							<Project-Git-Branch>${git.branch}</Project-Git-Branch>
							<Last-Commit-Timestamp>${git.commit.time}</Last-Commit-Timestamp>
							<Built-Timestamp>${git.build.time}</Built-Timestamp>
							<Built-By>${git.build.user.name}, ${git.build.user.email}</Built-By>
						</manifestEntries>
					</archive>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>${maven.war.plugin.version}</version>
				<configuration>
					<archive>
						<manifestEntries>
							<Organization>${pom.organization.name}</Organization>
							<Project>${pom.name}</Project>
							<Project-Version>${pom.version}.${git.commit.id.abbrev}</Project-Version>
							<Project-Git-Hash>${git.commit.id}</Project-Git-Hash>
							<Project-Git-Branch>${git.branch}</Project-Git-Branch>
							<Last-Commit-Timestamp>${git.commit.time}</Last-Commit-Timestamp>
							<Built-Timestamp>${git.build.time}</Built-Timestamp>
							<Built-By>${git.build.user.name}, ${git.build.user.email}</Built-By>
						</manifestEntries>
					</archive>
				</configuration>
			</plugin>


            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>versions-maven-plugin</artifactId>
                <version>2.1</version>
                <configuration>
                    <includes>
                        <include>org.openiam:*</include>
                    </includes>
                </configuration>
            </plugin>

		</plugins>

	</build>


